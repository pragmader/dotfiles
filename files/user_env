# Favourite editor
export EDITOR=emacs

# Go environment
export GOPATH="$HOME/go"
export PATH="$GOPATH/bin:$PATH"

# Custom scripts
if [ -d ~/.scripts ]; then
	export PATH="$PATH:$HOME/.scripts"
fi

# Local machine environment if exists
if [ -f ~/.user_env.local ]; then
	source ~/.user_env.local
fi
